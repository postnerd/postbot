{
  "name": "postbot",
  "version": "0.0.3-alpha",
  "description": "A TS chess engine",
  "type": "commonjs",
  "main": "compiled/main.js",
  "scripts": {
    "start": "node compiled/main.js",
    "watch": "rollup -c -w",
    "build": "pkg -c package.json compiled/main.js",
    "test": "jest --verbose"
  },
  "pkg": {
    "scripts": "compiled/chessWorker.js",
    "targets": [
      "node16-macos-x64",
      "node16-linux-x64",
      "node16-win-x64"
    ],
    "outputPath": "build"
  },
  "author": "postnerd",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.2",
    "pkg": "^5.7.0",
    "rollup": "^2.76.0",
    "ts-jest": "^28.0.5",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}