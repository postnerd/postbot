{
  "name": "postbot",
  "version": "0.1.0-beta_dev_moves",
  "description": "A TS chess engine",
  "type": "module",
  "main": "compiled/main.js",
  "scripts": {
    "start": "node compiled/main.js",
    "watch": "rollup -c -w",
    "build": "pkg -c package.json compiled/main.js",
    "test": "jest --verbose"
  },
  "pkg": {
    "scripts": "compiled/chessWorker.js",
    "targets": [
      "latest-macos-arm64",
      "latest-linux-arm64",
      "latest-win-arm64"
    ],
    "outputPath": "build"
  },
  "author": "postnerd",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-plugin-jest": "^27.4.2",
    "jest": "^29.7.0",
    "npm-check-updates": "^16.14.6",
    "pkg": "^5.8.1",
    "rollup": "^4.1.4",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "random-js": "^2.1.0"
  }
}